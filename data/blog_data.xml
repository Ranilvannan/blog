<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="blog_article_sequence" model="ir.sequence">
            <field name="name">Blog Article</field>
            <field name="code">blog.article</field>
            <field name="prefix">ARTICLE/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="blog_type_tech" model="blog.type">
            <field name="name">Tech Blog</field>
            <field name="code">TECH</field>
        </record>

        <record id="blog_type_tourist" model="blog.type">
            <field name="name">Tourist Blog</field>
            <field name="code">TECH</field>
        </record>

        <record id="blog_type_food" model="blog.type">
            <field name="name">Food Blog</field>
            <field name="code">FOOD</field>
        </record>

    </data>
</odoo>